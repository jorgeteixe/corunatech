---
import BaseLayout from './BaseLayout.astro'

interface Props {
  title?: string
  description?: string
  image?: string
}

const { title, description, image } = Astro.props
---

<BaseLayout title={title} description={description} image={image}>
  <div class="min-h-screen">
    <main>
      <slot />
    </main>
  </div>
</BaseLayout>
