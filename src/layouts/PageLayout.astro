---
import BaseLayout from './BaseLayout.astro'
import '../styles/layouts/PageLayout.css'

interface Props {
  title?: string
  description?: string
  image?: string
}

const { title, description, image } = Astro.props
---

<BaseLayout title={title} description={description} image={image}>
  <div class="page-layout">
    <main>
      <slot />
    </main>
  </div>
</BaseLayout>
